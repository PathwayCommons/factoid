{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true,
        "node": true,
        "mocha": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 8
    },
    "plugins": [

    ],
    "rules": {
      "semi": "error",
      "no-console": "warn",
      "no-unused-vars": "warn"
    }
}
