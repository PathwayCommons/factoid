{
  "name": "factoid",
  "private": true,
  "browserslist": "last 3 versions, >1%",
  "scripts": {
    "postinstall": "run-s build",
    "start": "forever ./src/server",
    "stop": "forever stop ./src/server",
    "lint:js": "eslint ./src",
    "fix:js": "eslint ./src --fix",
    "bundle:js": "webpack",
    "watch:js": "webpack --watch",
    "watch:sync-bundle": "browser-sync start --config browser-sync.config.js",
    "watch:sync-server": "nodemon src/server/index.js",
    "lint:css": "stylelint src/**/*.css",
    "fix:css": "stylelint src/**/*.css --fix",
    "bundle:css": "postcss src/styles/index.css -o build/bundle.css",
    "watch:css": "postcss --watch src/styles/index.css -o build/bundle.css",
    "bundle:css-deps": "postcss src/styles/deps.css -o build/deps.css",
    "watch:css-deps": "postcss --watch src/styles/deps.css -o build/deps.css",
    "bundle": "run-s bundle:*",
    "build": "run-p bundle",
    "build-prod": "cross-env NODE_ENV=production run-s build",
    "bundle-profile": "cross-env NODE_ENV=production PROFILE=true webpack --profile",
    "clean": "rimraf build/*",
    "lint": "run-s lint:*",
    "fix": "run-s fix:*",
    "watch": "run-p watch:*",
    "test": "mocha",
    "test:travis": "run-s test lint"
  },
  "dependencies": {
    "animejs": "^2.0.2",
    "bluebird": "^3.4.1",
    "body-parser": "^1.15.1",
    "clipboard": "^1.7.1",
    "cookie-parser": "^1.4.3",
    "cytoscape": "^3.2.7",
    "cytoscape-automove": "^1.9.0",
    "cytoscape-cose-bilkent": "^1.6.10",
    "cytoscape-cxtmenu": "^2.10.3",
    "cytoscape-edgehandles": "^3.2.1",
    "date-fns": "^1.28.5",
    "debug": "^2.2.0",
    "dice-coefficient": "^1.0.2",
    "eventemitter3": "^1.2.0",
    "express": "^4.13.4",
    "form-data": "^2.3.1",
    "fready": "^1.0.0",
    "hash.js": "^1.1.3",
    "heap": "^0.2.6",
    "hyperscript": "^2.0.2",
    "hyperscript-helpers": "^3.0.2",
    "leven": "^2.1.0",
    "lodash": "^4.13.1",
    "lru-cache": "^4.1.1",
    "morgan": "^1.7.0",
    "mousetrap": "^1.6.1",
    "node-fetch": "^2.1.1",
    "normalize.css": "^5.0.0",
    "popper.js": "^1.13.0",
    "query-string": "^4.3.4",
    "react": "^15.4.2",
    "react-anchor-link-smooth-scroll": "^1.0.5",
    "react-dom": "^15.4.2",
    "react-hyperscript": "^3.0.0",
    "react-router-dom": "^4.0.0",
    "react-tabs": "^1.1.0",
    "rethinkdb": "^2.3.2",
    "serve-favicon": "^2.3.0",
    "soap": "^0.23.0",
    "socket.io": "^1.4.8",
    "socket.io-client": "^1.4.8",
    "tippy.js": "^2.0.8",
    "uuid": "^2.0.2",
    "whatwg-fetch": "^2.0.3",
    "winston": "^2.3.1",
    "xml-js": "^1.6.2"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.9.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.16.0",
    "browser-sync": "^2.23.3",
    "chai": "^4.1.2",
    "cross-env": "^5.1.3",
    "cssnano": "^3.10.0",
    "eslint": "^4.15.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "forever": "^0.15.2",
    "mocha": "^3.5.3",
    "nodemon": "^1.14.10",
    "npm-run-all": "^4.1.2",
    "postcss": "^6.0.16",
    "postcss-cli": "^4.1.1",
    "postcss-cssnext": "^3.0.2",
    "postcss-import": "^10.0.0",
    "postcss-url": "^7.3.0",
    "rimraf": "^2.6.2",
    "stylelint": "^8.4.0",
    "stylelint-config-standard": "^17.0.0",
    "webpack": "^3.10.0",
    "webpack-bundle-analyzer": "^2.9.2"
  },
  "engines": {
    "node": ">=6.3.0"
  }
}
