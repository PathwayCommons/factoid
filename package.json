{
  "name": "factoid",
  "private": true,
  "browserslist": "edge >=12, firefox >=40, chrome >=40, safari 9",
  "scripts": {
    "postinstall": "run-s build",
    "start": "forever ./src/server",
    "stop": "forever stop ./src/server",
    "lint:js": "eslint ./src",
    "fix:js": "eslint ./src --fix",
    "bundle:js": "webpack",
    "watch:js": "webpack --watch",
    "watch:sync-bundle": "livereload \"build, src/views\"",
    "watch:sync-server": "nodemon src/server/index.js",
    "lint:css": "stylelint src/**/*.css",
    "fix:css": "stylelint src/**/*.css --fix",
    "bundle:css": "postcss src/styles/index.css -o build/bundle.css",
    "watch:css": "postcss --watch src/styles/index.css -o build/bundle.css",
    "bundle:css-deps": "postcss src/styles/deps.css -o build/deps.css",
    "watch:css-deps": "postcss --watch src/styles/deps.css -o build/deps.css",
    "bundle": "run-s bundle:*",
    "build": "run-p bundle",
    "build-prod": "cross-env NODE_ENV=production run-s build",
    "bundle-profile": "cross-env NODE_ENV=production PROFILE=true webpack --profile",
    "clean": "rimraf build/*",
    "lint": "run-s lint:*",
    "fix": "run-s fix:*",
    "watch": "run-p watch:*",
    "test": "mocha",
    "test:travis": "run-s ci",
    "ci": "run-s test lint"
  },
  "dependencies": {
    "animejs": "^3.0.1",
    "body-parser": "^1.15.1",
    "clipboard": "^2.0.4",
    "cookie-parser": "^1.4.3",
    "cytoscape": "^3.2.22",
    "cytoscape-automove": "^1.9.0",
    "cytoscape-cose-bilkent": "^4.0.0",
    "cytoscape-cxtmenu": "^3.0.2",
    "cytoscape-edgehandles": "^3.5.0",
    "cytoscape-popper": "^1.0.1",
    "date-fns": "^1.28.5",
    "debug": "^4.1.1",
    "dice-coefficient": "^1.0.2",
    "eventemitter3": "^3.1.0",
    "express": "^4.13.4",
    "file-saver": "^2.0.1",
    "form-data": "^2.3.1",
    "fready": "^1.0.0",
    "hash.js": "^1.1.3",
    "heap": "^0.2.6",
    "hyperscript": "^2.0.2",
    "hyperscript-helpers": "^3.0.2",
    "leven": "^2.1.0",
    "lodash": "^4.17.11",
    "lru-cache": "^5.1.1",
    "morgan": "^1.7.0",
    "mousetrap": "^1.6.1",
    "node-fetch": "^2.1.1",
    "nodemailer": "^5.1.1",
    "normalize.css": "^8.0.1",
    "p-cancelable": "^1.0.0",
    "popper.js": "^1.13.0",
    "query-string": "^6.3.0",
    "react": "^16.5.0",
    "react-anchor-link-smooth-scroll": "^1.0.5",
    "react-dom": "^16.5.0",
    "react-hyperscript": "^3.2.0",
    "react-router-dom": "^4.0.0",
    "rethinkdb": "^2.3.2",
    "serve-favicon": "^2.3.0",
    "soap": "^0.26.0",
    "socket.io": "^2.1.1",
    "socket.io-client": "^2.1.1",
    "tippy.js": "^4.0.4",
    "uuid": "^3.3.2",
    "whatwg-fetch": "^3.0.0",
    "winston": "^3.2.1",
    "xml-js": "^1.6.2"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.5",
    "babel-polyfill": "^6.9.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.16.0",
    "chai": "^4.1.2",
    "cross-env": "^5.1.3",
    "cssnano": "^4.1.10",
    "eslint": "^5.15.1",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "forever": "^0.15.2",
    "livereload": "^0.7.0",
    "mocha": "^6.0.2",
    "nodemon": "^1.18.7",
    "npm-run-all": "^4.1.2",
    "postcss": "^7.0.14",
    "postcss-cli": "^6.1.2",
    "postcss-cssnext": "^3.1.0",
    "postcss-import": "^12.0.1",
    "postcss-url": "^8.0.0",
    "rimraf": "^2.6.2",
    "stylelint": "^9.10.1",
    "stylelint-config-standard": "^18.2.0",
    "webpack": "^4.29.6",
    "webpack-bundle-analyzer": "^3.1.0"
  },
  "engines": {
    "node": ">=6.3.0"
  },
  "version": "0.4.1"
}
